{
  "name": "@bliztek/logger",
  "version": "1.0.0",
  "description": "Lightweight contextual logging service for Node & browser with runtime and env-based filtering.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "vitest run --coverage --reporter=junit --outputFile=test-report.junit.xml",
    "test:dev": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "logger",
    "typescript",
    "nextjs",
    "browser",
    "node",
    "contextual",
    "bliztek"
  ],
  "author": "Steven Brown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bliztek/logger.git"
  },
  "bugs": {
    "url": "https://github.com/bliztek/logger/issues"
  },
  "homepage": "https://github.com/bliztek/logger#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
